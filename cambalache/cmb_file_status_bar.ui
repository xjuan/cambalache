<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.99.8 -->
<interface>
  <!-- interface-name cmb_file_status_bar.ui -->
  <requires lib="gtk" version="4.18"/>
  <template class="CmbFileStatusBar" parent="GtkBox">
    <property name="orientation">vertical</property>
    <child>
      <object class="GtkRevealer" id="revealer">
        <property name="reveal-child">False</property>
        <child>
          <object class="GtkBox">
            <property name="css-classes">cmb-file-changed-bar</property>
            <property name="spacing">6</property>
            <child>
              <object class="GtkLabel" id="label">
                <property name="halign">start</property>
                <property name="hexpand">True</property>
                <property name="use-markup">True</property>
                <property name="wrap">True</property>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="button">
                <property name="valign">center</property>
                <signal name="clicked" handler="on_button_clicked"/>
              </object>
            </child>
            <child>
              <object class="GtkButton">
                <property name="css-classes">cmb-close</property>
                <property name="css-name">button</property>
                <property name="halign">center</property>
                <property name="icon-name">window-close-symbolic</property>
                <property name="valign">center</property>
                <signal name="clicked" handler="on_close_clicked"/>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
