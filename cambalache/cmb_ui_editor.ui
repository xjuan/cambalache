<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.97.9 -->
<interface>
  <!-- interface-name cmb_ui_property_editor.ui -->
  <!-- interface-copyright Juan Pablo Ugarte -->
  <requires lib="gtk" version="4.0"/>
  <template class="CmbUIEditor" parent="GtkBox">
    <property name="orientation">vertical</property>
    <property name="spacing">4</property>
    <child>
      <object class="GtkStackSwitcher">
        <property name="halign">center</property>
        <property name="stack">stack</property>
        <style>
          <class name="compact"/>
          <class name="property-pane"/>
        </style>
      </object>
    </child>
    <child>
      <object class="GtkStack" id="stack">
        <property name="vexpand">1</property>
        <child>
          <object class="GtkStackPage">
            <property name="child">
              <object class="CmbUIPropertyEditor" id="property_editor">
                <property name="hexpand">True</property>
                <property name="vexpand">True</property>
              </object>
            </property>
            <property name="name">properties</property>
            <property name="title" translatable="yes">Properties</property>
          </object>
        </child>
        <child>
          <object class="GtkStackPage">
            <property name="child">
              <object class="CmbUIRequiresEditor" id="requires_editor">
                <property name="orientation">vertical</property>
              </object>
            </property>
            <property name="name">requires</property>
            <property name="title" translatable="yes">Requires</property>
          </object>
        </child>
        <child>
          <object class="GtkStackPage">
            <property name="child">
              <object class="CmbFragmentEditor" id="fragment_editor">
                <property name="orientation">vertical</property>
              </object>
            </property>
            <property name="name">fragment</property>
            <property name="title">&lt;/&gt;</property>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
