<meta>
  <types>

    <AdwBanner>
      <properties>
        <property id="title" translatable="True"/>
      </properties>
    </AdwBanner>

    <AdwDialog>
      <properties>
        <property id="title" translatable="True"/>
      </properties>
    </AdwDialog>

    <AdwNavigationPage>
      <properties>
        <property id="title" translatable="True"/>
      </properties>
    </AdwNavigationPage>

    <AdwMessageDialog>
      <properties>
        <property id="heading" translatable="True"/>
        <property id="body" translatable="True"/>
      </properties>
    </AdwMessageDialog>

    <AdwHeaderBar>
      <children-types>
        <type max-children="1" linked-property-id="title-widget">title</type>
        <type>start</type>
        <type>end</type>
      </children-types>
    </AdwHeaderBar>

    <AdwFlap>
      <children-types>
        <type max-children="1" linked-property-id="flap">flap</type>
        <type max-children="1" linked-property-id="content">content</type>
        <type max-children="1" linked-property-id="separator">separator</type>
      </children-types>
    </AdwFlap>

    <AdwActionRow>
      <properties>
        <property id="icon-name" type="CmbIconName"/>
        <property id="subtitle" translatable="True"/>
      </properties>
    </AdwActionRow>

    <AdwAvatar>
      <properties>
        <property id="icon-name" type="CmbIconName"/>
      </properties>
    </AdwAvatar>

    <AdwButtonContent>
      <properties>
        <property id="icon-name" type="CmbIconName"/>
        <property id="label" translatable="True"/>
      </properties>
    </AdwButtonContent>

    <AdwExpanderRow>
      <properties>
        <property id="icon-name" type="CmbIconName"/>
        <property id="subtitle" translatable="True"/>
      </properties>
    </AdwExpanderRow>

    <AdwPreferencesPage>
      <properties>
        <property id="icon-name" type="CmbIconName"/>
        <property id="title" translatable="True"/>
      </properties>
    </AdwPreferencesPage>

    <AdwButtonRow>
      <properties>
        <property id="end-icon-name" type="CmbIconName"/>
        <property id="start-icon-name" type="CmbIconName"/>
      </properties>
    </AdwButtonRow>

    <AdwPreferencesRow>
      <properties>
        <property id="title" translatable="True"/>
      </properties>
    </AdwPreferencesRow>

    <AdwSplitButton>
      <properties>
        <property id="icon-name" type="CmbIconName"/>
        <property id="label" translatable="True"/>
        <property id="dropdown-tooltip" translatable="yes"/>
      </properties>
    </AdwSplitButton>

    <AdwStatusPage>
      <properties>
        <property id="icon-name" type="CmbIconName"/>
        <property id="description" translatable="True"/>
        <property id="title" translatable="True"/>
      </properties>
    </AdwStatusPage>

    <AdwViewStack>
      <children-constraints>
        <constraint shortcut="True">AdwViewStackPage</constraint>
      </children-constraints>
    </AdwViewStack>

    <AdwViewStackPage>
      <properties>
        <property id="child" required="True"
                  workspace-default="&lt;object class=&quot;GtkLabel&quot;&gt;&lt;property name=&quot;label&quot;&gt;Empty Page&lt;/property&gt;&lt;/object&gt;"/>
        <property id="icon-name" type="CmbIconName"/>
        <property id="title" translatable="True"/>
      </properties>
    </AdwViewStackPage>

    <AdwPreferencesGroup>
      <properties>
        <property id="description" translatable="True"/>
        <property id="title" translatable="True"/>
      </properties>
    </AdwPreferencesGroup>

    <AdwTabPage>
      <properties>
        <property id="title" translatable="True"/>
        <property id="tooltip" translatable="True"/>
      </properties>
    </AdwTabPage>

    <AdwToast>
      <properties>
        <property id="button-label" translatable="True"/>
        <property id="title" translatable="True"/>
      </properties>
    </AdwToast>

    <AdwToolbarView>
      <children-types>
        <type max-children="1">top</type>
        <type max-children="1">bottom</type>
      </children-types>
    </AdwToolbarView>

    <AdwViewSwitcherTitle>
      <properties>
        <property id="subtitle" translatable="True"/>
        <property id="title" translatable="True"/>
      </properties>
    </AdwViewSwitcherTitle>

    <AdwViewSwitcherTitle>
      <properties>
        <property id="subtitle" translatable="True"/>
        <property id="title" translatable="True"/>
      </properties>
    </AdwViewSwitcherTitle>

    <AdwWindowTitle>
      <properties>
        <property id="subtitle" translatable="True"/>
        <property id="title" translatable="True"/>
      </properties>
    </AdwWindowTitle>

    <AdwAboutDialog>
      <properties>
        <property id="title" translatable="True"/>
      </properties>
    </AdwAboutDialog>

    <AdwPreferencesDialog>
      <properties>
        <property id="title" translatable="True"/>
      </properties>
    </AdwPreferencesDialog>

    <AdwApplicationWindow>
      <properties>
        <property original-owner-id="GtkWindow" id="titlebar" disabled="True"/>
      </properties>
    </AdwApplicationWindow>

  </types>

  <categories>
    <category name="control">
      <AdwButtonRow/>
      <AdwComboRow/>
      <AdwEntryRow/>
      <AdwPasswordEntryRow/>
      <AdwSpinRow/>
      <AdwSplitButton/>
      <AdwSwitchRow/>
