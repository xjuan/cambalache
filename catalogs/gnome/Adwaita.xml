    <category name="display">
      <AdwAvatar/>
      <AdwBanner/>
      <AdwButtonContent/>
      <AdwCallbackAnimationTarget/>
      <AdwCarouselIndicatorDots/>
      <AdwCarouselIndicatorLines/>
      <AdwSpinner/>
      <AdwSpringAnimation/>
      <AdwTimedAnimation/>
      <AdwViewSwitcherTitle/>
      <AdwWindowTitle/>
    </category>

    <category name="layout">
      <AdwActionRow/>
      <AdwBin/>
      <AdwBottomSheet/>
      <AdwBreakpointBin/>
      <AdwCarousel/>
      <AdwClamp/>
      <AdwClampLayout/>
      <AdwClampScrollable/>
      <AdwExpanderRow/>
      <AdwFlap/>
      <AdwHeaderBar/>
      <AdwLayout/>
      <AdwLayoutSlot/>
      <AdwLeaflet/>
      <AdwLeafletPage/>
      <AdwMultiLayoutView/>
      <AdwNavigationPage/>
      <AdwNavigationSplitView/>
      <AdwNavigationView/>
      <AdwOverlaySplitView/>
      <AdwPreferencesGroup/>
      <AdwPreferencesPage/>
      <AdwPreferencesRow/>
      <AdwSqueezer/>
      <AdwSqueezerPage/>
      <AdwStatusPage/>
      <AdwTabBar/>
      <AdwTabPage/>
      <AdwTabView/>
      <AdwToast/>
      <AdwToastOverlay/>
      <AdwToolbarView/>
      <AdwViewStack/>
      <AdwViewStackPage/>
      <AdwViewSwitcherBar/>
    </category>

    <category name="model">
      <AdwEnumListItem/>
      <AdwEnumListModel/>
      <AdwViewStackPages/>
    </category>

    <category name="toplevel">
      <AdwAboutDialog/>
      <AdwAboutWindow/>
      <AdwAlertDialog/>
      <AdwApplicationWindow/>
      <AdwDialog/>
      <AdwMessageDialog/>
      <AdwPreferencesDialog/>
      <AdwPreferencesWindow/>
      <AdwWindow/>
    </category>
  </categories>

</meta>
  
